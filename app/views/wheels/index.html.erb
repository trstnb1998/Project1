<div class="backlinkindex">
    <%= link_to 'Go back', skateboards_path %>
</div>

<div class="header">
    <h1>All Wheels</h1>
    <%= link_to "New wheels", new_wheel_path(@wheel) %>
</div>  

<% @wheels.each do |wheel| %>
    <%= cl_image_tag(wheel.image, :class => 'thumb') if wheel.image.present? %>
    <ul>
        <li><%= link_to wheel.name, wheel %></li>
    </ul>
<% end %>