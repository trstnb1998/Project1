<div class="backlinkindex">
    <%= link_to 'Go back', skateboards_path %>
</div>

<div class="header">
    <h1>All decks</h1>
    <%= link_to "New deck", new_deck_path(@deck) %>
</div>   


<% @decks.each do |deck| %>
    <%=  cl_image_tag(deck.image, :class => 'thumb') if deck.image.present? %>
    <ul>
        <li><%= link_to deck.name, deck%></li>
    </ul>
<% end %>
